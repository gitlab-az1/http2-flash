{
  "name": "http2-flash",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/gitlab-az1/http2-flash.git",
  "author": "gitlab-az1 <gitlab.az1@gmail.com>",
  "license": "MPL-2.0",
  "keywords": [
    "http2",
    "framework",
    "flash",
    "http",
    "api",
    "node"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc --emitDeclarationOnly && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored && node ./post-build.js",
    "hot": "tsc --noEmit && yarn build && clear && node ./dist/index.js && rm -rf ./dist/",
    "test": "clear && yarn build && clear && node ./dist/test.js && rm -rf ./dist/",
    "spec": "clear && tsc --noEmit && jest --verbose --runInBand"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  }
}
